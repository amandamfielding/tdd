<h1>New User</h1>

<form action="/users" method="post">
  <%= auth_token_input %>

  <label>Username
    <input type="text" name="user[username]" value="">
  </label>

  <label>Password
    <input type="password" name="user[password]" value="">
  </label>

  <input type="submit" value="Submit">
</form>
